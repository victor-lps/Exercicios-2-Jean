programa {
  funcao inicio() {
    real n1, n2, n3

    escreva ("Escreva seu primeiro numero: ")
    leia (n1)
    escreva("Escreva seu segundo numero: ")
    leia (n2)
    escreva ("Escreva seu terceiro numero: ")
    leia (n3)

    se (n1 > n2 e n1 > n3)
    escreva ("O maior é: ", n1)

    senao se (n2 > n1 e n2 > n3)
    escreva ("O maior é: ",n2)

    senao se (n3 > n1 e n3 > n2)
    escreva ("O maior é: ", n3)







  }
}
